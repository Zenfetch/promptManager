{
  "name": "promptmanager",
  "displayName": "PromptManager",
  "description": "Easily modify prompts for LLMs directly in your IDE",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "homepage": "https://github.com/Zenfetch/promptManager/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/Zenfetch/promptManager/issues",
    "email": "gabe@zenfetch.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zenfetch/promptManager.git"
  },
  "publisher": "Zenfetch",
  "icon": "icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "promptmanager.showUI",
        "title": "Show Prompt Manager UI"
      },
      {
        "command": "promptmanager.helloWorld",
        "title": "Hello World"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "promptmanager.showUI"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "gpt-3-encoder": "^1.1.4",
    "protobufjs": "^7.2.3"
  }
}
